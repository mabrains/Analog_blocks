.option TEMP=85

*User_defined_functions
.control
define max(vector_name) (vecmax(vector_name))
define min(vector_name) (vecmin(vector_name))
.endc

*Temp_sweep
.control
dc temp 80 0 -1
let temp_coeff=1000000*(max(ldo_out)-min(ldo_out))/85
print temp_coeff
set wr_singlescale
set wr_vecnames
set appendwrite
wrdata ./corners_run/run_results/temp_sweep_18_85_2.2_fs temp_coeff
.endc
*supply_sweep
.control
dc Vs 3 0 -0.02
meas DC Vldo_Sup_2 FIND ldo_out AT=2
meas DC Vldo_Sup_3 FIND ldo_out AT=3
let line_reg=Vldo_Sup_3-Vldo_Sup_2
print line_reg
meas DC vin WHEN v(ldo_out)=1.764
let dropout=vin-1.764
print dropout
set wr_singlescale
set wr_vecnames
set appendwrite
wrdata ./corners_run/run_results/dc_sweep_18_85_2.2_fs dropout line_reg

.endc
*PSRR_Analysis
.control
alter Vs AC =1
ac dec 10 1 1G
meas AC PSRR100 FIND vdb(ldo_out) AT=100
meas AC PSRR100k FIND vdb(ldo_out) AT=100k
set appendwrite
set wr_singlescale
set wr_vecnames
wrdata ./corners_run/run_results/psrr_18_85_2.2_fs PSRR100 PSRR100k
.endc
